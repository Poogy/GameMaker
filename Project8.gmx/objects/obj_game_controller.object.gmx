         box.item = global.item; //Setzt den inhalt der box mit dem item das noch in der maus ist gleich
         global.item = -1; //Maus leeren
      } else {
         //Drop the item on the ground
         instance_create(mouse_x,mouse_y,global.item); //Item an der Stelle wo die maus ist ablegen
         global.item = -1; //Maus leeren
