            <kind>1</kind>
            <string>//Physics
var right_key = keyboard_check(ord('D'));
var right_arrow_key = keyboard_check(vk_right);
var left_key = keyboard_check(ord('A'));
var left_arrow_key = keyboard_check(vk_left);
var down_key = keyboard_check(ord('S'));
var jump_key = keyboard_check(ord('W'));

//Check Air/Ground
if place_meeting(x, y+1, obj_wall){
   vspd = 0;
   if jump_key {vspd-=jspd};
} else {
   if (vspd &lt; 10) {vspd+=grav}; 
}

//Movement
if place_meeting(x, y+1,obj_movingplatform){
   hspd = obj_movingplatform.spd;
} else { 
  if right_key {hspd = spd};
  if left_key {hspd = -spd};
  if (!right_key &amp;&amp; !left_key) || (right_key &amp;&amp; left_key) {hspd = 0};
}

//Collisions
if place_meeting(x+hspd, y,obj_wall){
   while !place_meeting(x+sign(hspd), y, obj_wall){
         x+=sign(hspd);
   }
   hspd = 0;
}
x+=hspd;

if place_meeting(x, y+vspd, obj_wall){
   while !place_meeting(x, y+sign(vspd), obj_wall){
         y+=sign(vspd);
   }
   vspd = 0;
}
y+=vspd;

//Leitern
if place_meeting(x, y,obj_ladder){
   vspd = 0;
   grav = 0;
} else {
   grav = grav_normal;
}

if (place_meeting(x, y,obj_ladder) &amp;&amp; jump_key){
   vspd = -cspd;
}
if (place_meeting(x, y,obj_ladder) &amp;&amp; down_key){
   vspd = cspd;
}
//Punch
if (right_arrow_key &amp;&amp; reload = 0) {
instance_create(x,y,obj_punch);
reload = 1
alarm[0]=30;
}

if (left_arrow_key &amp;&amp; reload = 0) {
instance_create(x,y,obj_punch);
reload = 1
alarm[0]=30;
}
</string>
